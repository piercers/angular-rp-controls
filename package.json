{
  "name": "angular-rp-controls",
  "version": "0.2.0",
  "main": "./bundles/rp-controls.umd.js",
  "module": "./index.js",
  "author": "Ryan S Pierce <ry@nspierce.com> (http://ryanspierce.com)",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "build": "npm run build.js && npm run build.umd && npm run build.uglify",
    "build.js": "ngc -p tsconfig.dist.json",
    "build.uglify": "uglifyjs -c --screw-ie8 --comments -o ./dist/bundles/rp-controls.min.umd.js ./dist/bundles/rp-controls.umd.js",
    "build.umd": "rollup -c rollup.config.js",
    "clean.post": "rimraf \"src/**/*.ngfactory.ts\" \"src/**/*.ngsummary.json\" \"index.ngsummary.json\"",
    "clean.pre": "rimraf dist",
    "copy": "cpy package.json README.md dist",
    "e2e": "protractor",
    "lint": "tslint \"src/**/*.ts\"",
    "ng": "ng",
    "postbuild": "npm run clean.post && npm run copy",
    "prepublish": "npm run build",
    "publish": "npm publish ./dist",
    "prebuild": "npm run clean.pre",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "start": "ng serve",
    "test": "ng test",
    "test.coverage": "ng test --code-coverage"
  },
  "peerDependencies": {
    "@angular/common": "^2.3.1",
    "@angular/core": "^2.3.1",
    "font-awesome": "^4.6.3",
    "lodash": "^4.16.4",
    "rxjs": "^5.0.1",
    "simplemde": "^1.11.2"
  },
  "devDependencies": {
    "@angular/compiler": "^2.3.1",
    "@angular/compiler-cli": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "@types/simplemde": "^1.11.5",
    "angular-cli": "1.0.0-beta.24",
    "codelyzer": "~2.0.0-beta.1",
    "core-js": "^2.4.1",
    "cpy-cli": "^1.0.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "rimraf": "^2.5.4",
    "rollup": "^0.34.13",
    "ts-helpers": "^1.1.1",
    "ts-node": "1.2.1",
    "tslint": "^4.0.2",
    "typescript": "~2.0.3",
    "uglifyjs": "^2.4.10",
    "zone.js": "^0.7.2"
  }
}
